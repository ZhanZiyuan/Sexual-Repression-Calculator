{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "functions": {
        "api/**/*.[jt]s": {
            "runtime": "vercel-deno@3.1.0"
        }
    },
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}