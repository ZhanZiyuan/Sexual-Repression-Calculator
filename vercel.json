{
    "version": 3,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/web"
            }
        },
        {
            "src": "dist/server.cjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/dist/server.cjs"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}