{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        },
        {
            "src": "src/server/**/*.ts",
            "use": "@vercel/deno"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/server/$1.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}