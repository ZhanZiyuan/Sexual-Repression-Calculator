{
    "version": 3,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/web"
            }
        },
        {
            "src": "src/server/**/*.ts",
            "use": "@vercel/deno-edge"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/server/$1.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "functions": {
        "src/server/**/*.ts": {
            "runtime": "vercel-deno@3.1.0"
        }
    }
}