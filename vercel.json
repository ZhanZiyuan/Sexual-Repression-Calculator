{
    "version": 2,
    "functions": {
        "src/server/**/*.ts": {
            "runtime": "edge"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/server/$1.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}