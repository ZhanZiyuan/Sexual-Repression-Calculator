{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "functions": {
        "api/**/*.[jt]s": {
            "runtime": "vercel-deno@3.1.0"
        }
    },
    "buildCommand": "npm run build",
    "outputDirectory": "dist/web",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/web/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/dist/web/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}